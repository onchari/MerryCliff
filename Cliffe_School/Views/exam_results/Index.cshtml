@model IEnumerable<Cliffe_School.Models.exam_results>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.marks)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.course.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.student.email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.marks)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.course.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.student.email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.exam_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.exam_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.exam_id })
        </td>
    </tr>
}

</table>
